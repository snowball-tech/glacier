    "quality": "yarn run format",
    "quality-fix": "yarn run format-fix",
    "update-deps": "yarn run -T update-deps",
    "validate-config": "renovate-config-validator default.json5"
  },
  "devDependencies": {
    "renovate": "^37.388.1"
  }
