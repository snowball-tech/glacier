{
  "name": "@snowball-tech/utils",
  "version": "0.0.0-development",
  "description": "Utils library for Snowball packages and apps",
  "author": "Snowball Engineering Team <tech@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "private": true,
  "license": "MIT",
  "keywords": [
    "snowball",
    "utils",
    "typescript"
  ],
  "homepage": "https://github.com/snowball-tech/glacier/tree/main/packages/utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/glacier.git",
    "directory": "packages/utils"
  },
  "bugs": "https://github.com/snowball-tech/glacier/issues",
  "packageManager": "yarn@3.5.1",
  "engines": {
    "yarn": ">= 3.5.1",
    "node": ">= 18.16.0"
  },
  "config": {},
  "publishConfig": {
    "access": "restricted"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run -T tsc --build",
    "format": "yarn run -T prettier --ignore-path ../../.prettierignore --check .",
    "format:fix": "yarn run -T prettier --ignore-path ../../.prettierignore --write .",
    "lint": "yarn run -T eslint --cache .",
    "lint:fix": "yarn run -T eslint --cache --fix .",
    "types:check": "yarn run -T tsc --noEmit"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.5.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.5",
    "@types/read-pkg-up": "^6.0.0",
    "@types/semver": "^7.5.0"
  }
}
