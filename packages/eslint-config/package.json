{
  "name": "@snowball-tech/eslint-config",
  "version": "1.0.1",
  "description": "ESLint ruleset by Snowball supporting React & Next.JS, Typescript, Prettier and more, based on Airbnb config",
  "author": "Snowball Engineering Team <tech@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "airbnb",
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "react",
    "nextjs",
    "prettier",
    "jest",
    "testing-library",
    "lodash"
  ],
  "homepage": "https://github.com/snowball-tech/glacier/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/glacier.git",
    "directory": "packages/eslint-config"
  },
  "bugs": "https://github.com/snowball-tech/glacier/issues",
  "packageManager": "yarn@3.5.1",
  "engines": {
    "yarn": ">= 3.5.1",
    "node": ">= 18.16.0"
  },
  "main": ".eslintrc.js",
  "files": [
    "patch.js"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "format": "yarn run -T prettier --ignore-path ../../.prettierignore --check .",
    "format:fix": "yarn run -T prettier --ignore-path ../../.prettierignore --write .",
    "lint": "yarn run -T eslint --cache .",
    "lint:fix": "yarn run -T eslint --cache --fix ."
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@snowball-tech/utils": "1.0.1",
    "@typescript-eslint/eslint-plugin": "~5.59.6",
    "@typescript-eslint/parser": "~5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json5": "^0.1.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-yml": "^1.7.0"
  }
}
