{
  "name": "@snowball-tech/eslint-config",
  "version": "3.1.3",
  "description": "ESLint ruleset by Snowball supporting React & Next.JS, Typescript, Prettier and more, based on Airbnb config",
  "author": "Snowball Engineering Team <engineering@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "airbnb",
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "react",
    "nextjs",
    "prettier",
    "jest",
    "testing-library",
    "lodash"
  ],
  "homepage": "https://github.com/snowball-tech/glacier/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/glacier.git",
    "directory": "packages/eslint-config"
  },
  "bugs": "https://github.com/snowball-tech/glacier/issues",
  "packageManager": "yarn@3.6.0",
  "engines": {
    "yarn": "^3.0.0",
    "node": "^18.0.0"
  },
  "main": "index.js",
  "files": [
    "base.js",
    "full.js",
    "html.js",
    "import.js",
    "jest.js",
    "json.js",
    "lodash.js",
    "markdown.js",
    "next.js",
    "patch.js",
    "perfectionist.js",
    "prettier.js",
    "react.js",
    "secrets.js",
    "sql.js",
    "testing-library.js",
    "typescript.js",
    "yml.js"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "find-rules": "eslint-find-rules",
    "format": "yarn run -T prettier --ignore-path ../../.prettierignore --cache --check .",
    "format:fix": "yarn run -T prettier --ignore-path ../../.prettierignore --cache --write .",
    "lint": "yarn run -T eslint --cache .",
    "lint:fix": "yarn run -T eslint --cache --fix ."
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^13.4.7",
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/dom": "^9.3.1",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-perfectionist": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-sql": "^2.3.2",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-yml": "^1.8.0",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.3",
    "eslint-find-rules": "^4.1.0"
  }
