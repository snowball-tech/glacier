{
  "name": "@snowball-tech/fractal",
  "version": "1.0.0",
  "description": "Snowball's Design System",
  "author": "Snowball Engineering Team <engineering@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "design-system",
    "fractal",
    "react",
    "styled-components"
  ],
  "homepage": "https://github.com/snowball-tech/glacier/tree/main/packages/fractal",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/glacier.git",
    "directory": "packages/fractal"
  },
  "bugs": "https://github.com/snowball-tech/glacier/issues",
  "packageManager": "yarn@3.6.0",
  "engines": {
    "yarn": "^3.0.0",
    "node": "^18.0.0"
  },
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/fractal.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fractal.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prebuild": "yarn run clean && turbo run update:fonts",
    "prebuild:fast": "yarn run clean",
    "build": "yarn run prebuild && tsc && vite build",
    "build:fast": "yarn run prebuild:fast && tsc && vite build",
    "build:storybook": "yarn run prebuild && storybook build",
    "predev": "turbo run update:fonts",
    "dev": "yarn run predev && storybook dev -p 6006",
    "clean": "rm -Rf dist",
    "format": "yarn run -T prettier --ignore-path ../../.prettierignore --check .",
    "format:fix": "yarn run -T prettier --ignore-path ../../.prettierignore --write .",
    "lint": "yarn run -T eslint --cache .",
    "lint:fix": "yarn run -T eslint --cache --fix .",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "preview": "vite preview",
    "types:check": "tsc --noEmit",
    "preupdate:fonts": "rm -Rf assets/fonts/*",
    "update:fonts": "yarn run preupdate:fonts && cp ../../node_modules/@snowball-tech/design-tokens/dist/web/css/fonts/* ./assets/fonts/ 2>&1 || true"
  },
  "devDependencies": {
    "@snowball-tech/eslint-config": "2.2.1",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-storysource": "^7.0.20",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/addons": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/components": "^7.0.20",
    "@storybook/manager-api": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.20",
    "@types/eslint": "^8.40.2",
    "@types/lodash": "^4.14.195",
    "@types/prettier": "^2.7.3",
    "@types/prop-types": "^15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.42.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "install": "^0.13.0",
    "json": "^11.0.0",
    "npm": "^9.7.1",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.20",
    "storybook-addon-mock": "^4.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@snowball-tech/design-tokens": "1.3.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3"
  }
}
